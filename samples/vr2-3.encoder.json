{
    "Buffers": [
        {
            "V-Trace": "./samples/V-Trace-left.csv",
            "buffer": "left"
        },
        {
            "V-Trace": "./samples/V-Trace-right.csv",
            "buffer": "right"
        }
    ],
    "Bitrate": {
        "mode": "CBR",
        "bitrate": "30000000",
        "window_framerate": "1",
        "QPmin": -1,
        "QPmax": -1
    },
    "ErrorResilience": {
        "mode": "pIntra",
        "parameter": "1"
    },
    "Slice": {
        "mode": "fixed",
        "parameter": "8"
    },
    "PreDelay": {
            "mode": "equally",
            "parameter1": "10",
            "parameter2": "30"
    },
    "EncodingDelay": {
            "mode": "GaussianTrunc",
            "parameter1": "8",
            "parameter2": "3",
            "parameter3": "1"
    },
    "frame_width": 2048,
    "frame_height": 2048,
    "frame_rate": 60,
    "start_frame": 0,
    "total_frames": 3600,
    "buffer_interleaving": false,
    "S-Trace": "./samples_results/S-Trace.csv"
}
